{
	"version": "2.0.0",
	"command": "npm",
	"tasks": [
		{
			"label": "install (production build)",
			"type": "shell",
			"args": [
				"install",
				"--silent"
			],
			"problemMatcher": [
				"$tsc"
			]
		},
		{
			"label": "development build",
			"type": "shell",
			"args": [
				"run",
				"build-debug",
				"--silent"
			],
			"problemMatcher": [
				"$tsc"
			],
			"group": {
				"_id": "build",
				"isDefault": false
			}
		}
	]
}
