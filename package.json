{
	"name": "crosscall",
	"version": "0.0.6-dev.0",
	"description": "cross-origin postmessage rpc",
	"author": "Chase Moskal <chasemoskal@gmail.com>",
	"license": "ISC",
	"keywords": [
		"cross-origin",
		"cross-domain",
		"postmessage",
		"rpc",
		"iframe",
		"rpc",
		"bridge",
		"xorigin",
		"xdomain"
	],
	"main": "dist",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"source"
	],
	"scripts": {
		"prepare": "run-s build-production test",
		"build-production": "rimraf dist && tsc",
		"build-debug": "rimraf dist && tsc --target esnext --sourceMap false --inlineSourceMap true",
		"coverage": "npm run build-debug -s && jest --verbose --coverage",
		"start": "http-server",
		"test": "jest --verbose"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chase-moskal/crosscall.git"
	},
	"bugs": {
		"url": "https://github.com/chase-moskal/crosscall/issues"
	},
	"homepage": "https://github.com/chase-moskal/crosscall#readme",
	"devDependencies": {
		"@types/jest": "23.3.13",
		"http-server": "0.11.1",
		"jest": "23.6.0",
		"mock-local-storage": "1.1.8",
		"npm-run-all": "4.1.5",
		"requirejs": "2.3.6",
		"rimraf": "2.6.3",
		"typescript": "3.2.4"
	}
}
