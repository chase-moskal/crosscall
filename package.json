{
	"name": "crosscall",
	"version": "0.0.6-dev.11",
	"description": "cross-origin postmessage rpc",
	"author": "Chase Moskal <chasemoskal@gmail.com>",
	"license": "ISC",
	"keywords": [
		"cross-origin",
		"cross-domain",
		"postmessage",
		"rpc",
		"iframe",
		"rpc",
		"bridge",
		"xorigin",
		"xdomain"
	],
	"files": [
		"dist",
		"source"
	],
	"scripts": {
		"prepare": "run-s build test",
		"build": "rimraf dist && mkdirp dist dist/cjs && run-p build-esm build-cjs",
		"build-esm": "tsc",
		"build-cjs": "tsc --module commonjs --outDir dist/cjs",
		"start": "serve",
		"test": "jest --verbose dist/cjs"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chase-moskal/crosscall.git"
	},
	"bugs": {
		"url": "https://github.com/chase-moskal/crosscall/issues"
	},
	"homepage": "https://github.com/chase-moskal/crosscall#readme",
	"devDependencies": {
		"@types/jest": "^24.0.18",
		"jest": "^24.9.0",
		"mkdirp": "^0.5.1",
		"mock-local-storage": "^1.1.8",
		"npm-run-all": "^4.1.5",
		"requirejs": "^2.3.6",
		"rimraf": "^3.0.0",
		"serve": "^11.2.0",
		"typescript": "^3.6.3"
	}
}
